<?xml version="1.0" encoding="UTF-8"?>
<Configuration
    status="WARN"
    shutdownHook="enable"
>
    <Appenders>
        <Console
            name="Console"
            target="SYSTEM_OUT"
        >
            <PatternLayout pattern="%-5p|%d{ISO8601}{GMT}|%c{1}|%m%ex%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="WARN">
            <AppenderRef ref="Console" />
        </Root>
        <Logger
            name="org.greeneyed"
            level="INFO" />
    </Loggers>
</Configuration>
